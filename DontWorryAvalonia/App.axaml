<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="DontWorryAvalonia.App"
             xmlns:local="using:DontWorryAvalonia"
             RequestedThemeVariant="Dark">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

	<!--The view locator can therefore always be found by Avalonia UI, 
	        when it searches for a data template.-->
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
		<StyleInclude Source="/Icons/Icons.axaml" />
    </Application.Styles>
</Application>